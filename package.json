{
  "name": "shamba-website",
  "version": "1.0.0",
  "description": "Shamba.land - Geospatial Smallholder CRM System",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w frontend\" \"npm run develop -w backend\"",
    "dev:frontend": "npm run dev -w frontend",
