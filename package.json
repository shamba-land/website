    "build": "npm run build -w frontend && npm run build -w backend"
  },
  "keywords": [
    "shamba",
    "crm",
    "geospatial"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
